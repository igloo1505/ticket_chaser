        "@types/react": "18.2.15",
        "@types/react-dom": "18.2.7",
        "@types/react-redux": "^7.1.25",
        "animate.css": "^4.1.1",
        "axios": "^1.4.0",
        "bcrypt": "^5.1.0",
        "check-password-strength": "^2.0.7",
        "clsx": "^2.0.0",
        "dayjs": "^1.11.9",
        "eslint": "8.45.0",
        "eslint-config-next": "13.4.12",
        "firebase": "^10.1.0",
        "firebase-admin": "^11.10.1",
        "formk": "^0.0.20",
        "gsap": "^3.12.2",
